{
  "name": "xcaret-to-do",
  "version": "0.1.0",
  "scripts": {
    "build:styles": "tailwind build src/styles/index.scss -o ./public/css/tailwind.css",
    "build:css": "postcss src/**/*.scss -o public/css/index.css && npm run build:styles",
    "watch:css": "postcss src/**/*.scss -o src/index.css -w",
    "start": "npm-run-all -p build:styles start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@fortawesome/fontawesome-svg-core": "^1.2.26",
    "@fortawesome/free-brands-svg-icons": "^5.12.0",
    "@fortawesome/free-regular-svg-icons": "^5.12.0",
    "@fortawesome/free-solid-svg-icons": "^5.12.0",
    "@fortawesome/react-fontawesome": "^0.1.8",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.4.0",
    "@testing-library/user-event": "^7.2.1",
    "apollo-boost": "^0.4.7",
    "axios": "^0.21.1",
    "graphql": "^14.6.0",
    "node-sass": "^4.13.1",
    "prop-types": "^15.7.2",
    "react": "^16.12.0",
    "react-device-detect": "^1.11.14",
    "react-dom": "^16.12.0",
    "react-scripts": "3.3.1"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "postcss-nested": "^4.2.1",
    "tailwindcss": "^1.1.4"
  }
}
